<Window x:Class="Bulletin_impot.bareme"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bulletin_impot"
        mc:Ignorable="d"
        Title="bareme" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Height="269" HorizontalAlignment="Left" Name="editeur" TextChanged="Editeur_KeyDown"  VerticalAlignment="Top" Width="503" AcceptsReturn="True" />
        <Button Content="Ouvrir" Height="29" HorizontalAlignment="Left" Margin="0,270,0,0" Name="boutonOuvrir" VerticalAlignment="Top" Width="96" Click="BoutonOuvrir_Click" />
        <Button Content="Enregistrer" Height="29" HorizontalAlignment="Left" Margin="102,270,0,0" Name="boutonEnregistrer" VerticalAlignment="Top" Width="96" Click="BoutonEnregistrer_Click" />
    </Grid>
</Window>
